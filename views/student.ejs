<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>student page</title>
  </head>
  <body>
    <a href="/students/add">Add Student</a><br><br>
    <table border="1px" width="100%">
      <tr>
        <th>id</th>
        <th>first name</th>
        <th>last name</th>
        <th>email</th>
        <th>jurusan</th>
        <th>actions</th>
      </tr>
      <% dataStudent.forEach(d => { %>
      <tr>
        <td><%= d.id %></td>
        <td><%= d.first_name %></td>
        <td><%= d.last_name %></td>
        <td><%= d.email %></td>
        <td><%= d.jurusan %></td>
        <td><a href="/students/edit/<%= d.id %>">Edit</a> | <a href="/students/delete/<%= d.id %>" onclick="return confirm('Delete this student?')">Delete</a></td>
      </tr>
      <% }) %>
    </table>
    <br><a href="/">back</a>
  </body>
</html>
