<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>subject page</title>
  </head>
  <body>
    <table border="1px" width="100%">
      <tr>
        <th>id</th>
        <th>subject name</th>
        <th>teacher</th>
        <th>actions</th>
      </tr>
      <% dataSubject.forEach(d => { %>
      <tr>
        <td><%= d.id %></td>
        <td><%= d.subject_name %></td>
        <td>
          <ol>
            <%d.Teachers.forEach(teacher => {%>
            <li><%= teacher.first_name %> <%= teacher.last_name %></li>
            <%})%>
          </ol>
        </td>
        <td><a href="subjects/<%=d.id%>/enrolledstudents">Enrolled Students</a></td>
      </tr>
      <% }) %>
    </table>
    <br><a href="/">back</a>
  </body>
</html>
